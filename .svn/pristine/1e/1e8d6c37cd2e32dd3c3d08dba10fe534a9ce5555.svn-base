<?xml version="1.0" encoding="utf-8"?>
<!--
   drawable define by Ravi for change custom underline on focus for Blue Color
-->

<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:bottom="1dp"
        android:left="-2dp"
        android:right="-2dp"
        android:top="-2dp">
        <shape android:shape="rectangle" >
            <stroke
                android:width="1dp"
                android:color="@color/sdk_seconderycolor" />
        </shape>
    </item>
</layer-list>