package com.empass.sdk.vo;

public class LeaderVO {                                                          //Class for data array in response of Leaderboard @author Rockstar
    private String playerId;                                                     // variable for player id
    private String rank;                                                         // variable for rank
    private String country;                                                      //variable for country
    private String playerName;                                                   //variable for player name
    private String cLevel;                                                       // variable for clevel
    private String cSubLevel;                                                    //variable for csublevel
    private String avgScore;
    private String ccName;                                                     // variable for avgScore

    public String getDomainColor() {
        return domainColor;
    }

    public void setDomainColor(String domainColor) {
        this.domainColor = domainColor;
    }

    private String domainColor;

    public String getImage() {
        return image;
    }

    public void setImage(String image) {
        this.image = image;
    }

    public String getCcName() {
        return ccName;
    }

    public void setCcName(String ccName) {
        this.ccName = ccName;
    }

    private String image;                                                     // variable for avgScore

    // variable for avgScore
    public int getDomainId() {
        return domainId;
    }

    public void setDomainId(int domainId) {
        this.domainId = domainId;
    }

    private int domainId;

    public LeaderVO() {                                                          //Class Constructor
        super();
    }

    @Override
