        return "LeaderVO [playerId=" + playerId + ", rank=" + rank
                + ", country=" + country + ", playerName=" + playerName
                + ", cLevel=" + cLevel + ", cSubLevel=" + cSubLevel
                + ", avgScore=" + avgScore + "]";
    }

    public String getPlayerId() {                                            // Method to get player id @return
        return playerId;
    }

    public void setPlayerId(String playerId) {                                            //Method to set player id @return
        this.playerId = playerId;
    }

    public String getRank() {                                                              //Method to get rank @return
        return rank;
    }

    public void setRank(String rank) {                                                    //Method to set rank @return
        this.rank = rank;
    }

    public String getCountry() {                                                          //Method to get country @return
        return country;
    }

    public void setCountry(String country) {                                             //Method to set country @return
        this.country = country;
    }

    public String getPlayerName() {                                                     //Method to get player name @return
        return playerName;
    }

    public void setPlayerName(String playerName) {                                      // Method to set player name @return
        this.playerName = playerName;
    }

    public String getcLevel() {                                                        //Method to get cLevel @return
        return cLevel;
    }

    public void setcLevel(String cLevel) {                                             //Method to set clevel @return
        this.cLevel = cLevel;
    }

    public String getcSubLevel() {                                                   //Method to get csublevel @return
        return cSubLevel;
    }

    public void setcSubLevel(String cSubLevel) {                                     //Method to set csublevel @return
