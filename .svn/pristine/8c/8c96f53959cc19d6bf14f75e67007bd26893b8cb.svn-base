package com.empass.sdk.vo;
import com.google.gson.annotations.SerializedName;

import java.util.ArrayList;

public class UserQuestionVO {
	@SerializedName("goalId")
	private String goalId;
	@SerializedName("questionNo")   //declear variable
	private String questionNo;
	@SerializedName("question")
	private String question;
	@SerializedName("player1Score")
	private int player1Score;
	@SerializedName("player2Score")
	private int player2Score;
	@SerializedName("player1Time")
	private int player1Time;
	@SerializedName("player2Time")
	private int player2Time;
	@SerializedName("playerCLevel")
	private String playerCLevel;
	@SerializedName("playerCSubLevel")
	private String playerCSubLevel;
	@SerializedName("options")
	private ArrayList<UserOption> options;
	public UserQuestionVO() {
		super();
	}
	public String getQuestionNo() {
		return questionNo;
	}
	public void setQuestionNo(String questionNo) {
		this.questionNo = questionNo;
	}
	public String getQuestion() {
		return question;
	}
	public void setQuestion(String question) {
		this.question = question;
	}
	public ArrayList<UserOption> getOptions() {
		return options;
	}
	public void setOptions(ArrayList<UserOption> options) {
		this.options = options;
	}
	public int getPlayer1Score() {
		return player1Score;
	}
	public void setPlayer1Score(int player1Score) {
		this.player1Score = player1Score;
	}
	public int getPlayer2Score() {
		return player2Score;
	}
	public void setPlayer2Score(int player2Score) {
		this.player2Score = player2Score;
	}
	public int getPlayer1Time() {
		return player1Time;
	}
	public void setPlayer1Time(int player1Time) {
		this.player1Time = player1Time;
	}
	public int getPlayer2Time() {
		return player2Time;
	}
	public void setPlayer2Time(int player2Time) {
		this.player2Time = player2Time;
	}
	public String getPlayerCLevel() {
		return playerCLevel;
	}
	public void setPlayerCLevel(String playerCLevel) {
		this.playerCLevel = playerCLevel;
	}
	public String getPlayerCSubLevel() {
		return playerCSubLevel;
	}
	public void setPlayerCSubLevel(String playerCSubLevel) {
		this.playerCSubLevel = playerCSubLevel;
	}
	public String getGoalId() {
		return goalId;
	}
	public void setGoalId(String goalId) {
		this.goalId = goalId;
	}
}
